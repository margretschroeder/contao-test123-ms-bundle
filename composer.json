{
    "name": "margretschroeder/contao-test123-ms-bundle",
    "type": "contao-bundle",
    "description": "Test f√ºr Margret von Contao 4 bundles, V2",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Margret Schroeder",
            "homepage": "https://margretschroeder.de"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.7.*",
        "symfony/framework-bundle": "^3.3"
        
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Margretschroeder\\ContaoTest123MsBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Margretschroeder\\ContaoTest123MsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Margretschroeder\\ContaoTest123MsBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/margretschroeder/contao-test123-ms-bundle/issues",
        "source": "https://github.com/margretschroeder/contao-test123-ms-bundle"
    }
}
